<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.LoginController">
    <children>
        <TitledPane animated="false" layoutX="90.0" layoutY="11.0" prefHeight="378.0" prefWidth="420.0" text="Login">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                    <children>
                        <TextField fx:id="usernameField" layoutX="151.0" layoutY="93.0" />
                        <Label layoutX="61.0" layoutY="94.0" text="Username" />
                        <Label layoutX="61.0" layoutY="136.0" text="Password" />
                        <Button fx:id="loginButton" layoutX="180.0" layoutY="248.0" mnemonicParsing="false" onAction="#handleLoginButtonAction" prefHeight="25.0" prefWidth="91.0" text="Login" />
                        <Button fx:id="registerButton" layoutX="181.0" layoutY="283.0" mnemonicParsing="false" onAction="#handleOnRegister" prefHeight="25.0" prefWidth="89.0" text="Register" />
                        <PasswordField fx:id="passwordField" layoutX="151.0" layoutY="135.0" />
                    </children>
                </AnchorPane>
            </content>
        </TitledPane>
    </children>
</AnchorPane>
